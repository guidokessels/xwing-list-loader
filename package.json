{
  "name": "xwing-list-loader",
  "version": "0.0.0-development",
  "description": "Load lists in xws format from all popular X-Wing list builders",
  "main": "dist/index.js",
  "author": "Guido Kessels <guidokessels@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "babel lib -d dist",
    "test": "jest tests",
    "test:watch": "jest --watch",
    "semantic-release": "semantic-release"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "printWidth": 100
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add",
      "jest --bail --findRelatedTests"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "cross-fetch": "^3.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@commitlint/travis-cli": "^8.0.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "husky": "^2.5.0",
    "jest": "^22.0.4",
    "lint-staged": "^8.2.1",
    "nock": "^10.0.6",
    "prettier": "^2.0.0",
    "semantic-release": "^15.13.17"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guidokessels/xwing-list-loader.git"
  }
}
